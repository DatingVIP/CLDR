{
	"name": "datingvip/cldr",
	"type": "library",
	"description": "An API to localize applications using Unicode's CLDR",
	"keywords": [
		"i18n",
		"cldr",
		"locale",
		"territory",
		"currency",
		"format"
	],
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/DatingVIP/CLDR/issues",
		"source": "https://github.com/DatingVIP/CLDR"
	},
	"config": {
		"sort-packages": true
	},
	"require": {
		"php": ">=7.1",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"icanboogie/common": "^1.3|^2.0|^6.0",
		"icanboogie/accessor": "^3.0|^4.0"
	},
	"require-dev": {
		"ext-redis": "*",
		"phpstan/phpstan": "^1.4",
		"phpunit/phpunit": "^7.5.20|^8.5"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\CLDR\\": "lib"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ICanBoogie\\CLDR\\": "tests"
		}
	}
}
