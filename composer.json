{
	"name": "icanboogie/cldr",
	"type": "library",
	"description": "An API to localize applications using Unicode's CLDR",
	"keywords": [
		"i18n",
		"cldr",
		"locale",
		"territory",
		"currency",
		"format"
	],
	"homepage": "http://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "http://www.weirdog.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/ICanBoogie/CLDR/issues",
		"source": "https://github.com/ICanBoogie/CLDR"
	},
	"require": {
		"php": ">=5.5.0",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"icanboogie/common": "^1.2.2",
		"icanboogie/accessor": "^1.0.2",
		"icanboogie/storage": "^1.1.1|^2.0.0"
	},
	"require-dev": {
		"icanboogie/datetime": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\CLDR\\": "lib"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ICanBoogie\\CLDR\\": "tests"
		}
	}
}
