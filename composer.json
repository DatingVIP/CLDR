{
	"name": "datingvip/cldr",
	"type": "library",
	"description": "An API to localize applications using Unicode's CLDR",
	"keywords": [
		"i18n",
		"cldr",
		"locale",
		"territory",
		"currency",
		"format"
	],
	"homepage": "https://icanboogie.org/",
	"license": "BSD-3-Clause",
	"authors": [
		{
			"name": "Olivier Laviale",
			"email": "olivier.laviale@gmail.com",
			"homepage": "https://olvlvl.com/",
			"role": "Developer"
		}
	],
	"support": {
		"issues": "https://github.com/DatingVIP/CLDR/issues",
		"source": "https://github.com/DatingVIP/CLDR"
	},
	"require": {
		"php": ">=5.6",
		"ext-curl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"datingvip/common": "^1.2|^2.0",
		"datingvip/accessor": "^1.0|^2.0|^3.0|^4.0"
	},
	"require-dev": {
		"ext-redis": "*"
	},
	"autoload": {
		"psr-4": {
			"ICanBoogie\\CLDR\\": "lib"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"ICanBoogie\\CLDR\\": "tests"
		}
	}
}
